{
  "$schema": "https://swc.rs/schema.json",
  "sourceMaps": true,
  "module": {
    "type": "commonjs"
  },
  "exclude": [
    "node_modules",
    "dist",
    "test",
    "__tests__",
    "coverage",
    ".*\\.spec\\.[tj]s$",
    ".*\\.test\\.[tj]s$",
    ".*\\.e2e-spec\\.[tj]s$",
    ".*\\.map$"
  ],
  "jsc": {
    "target": "es2024",
    "baseUrl": "./",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": false
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true,
    "minify": {
      "compress": true
    }
  },
  "minify": true
}
