{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [{ "name": "react" }],
    "strict": true,
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": ["DOM", "DOM.Iterable", "ES2024"],
    "isolatedModules": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "target": "ES2024",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true
  }
}
