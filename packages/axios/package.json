{
  "name": "@workspace/axios",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "files": ["dist"],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "biome lint ."
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.9",
    "@workspace/schema": "workspace:*",
    "axios": "^1.13.2",
    "cookie-es": "^2.0.0",
    "nanoid": "^5.1.6",
    "pino": "^10.1.0",
    "qs": "^6.14.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/qs": "^6.14.0",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@vitest/ui": "^4.0.9",
    "axios-mock-adapter": "^2.1.0",
    "tsdown": "^0.16.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "import": "./dist/api/index.mjs",
      "require": "./dist/api/index.js"
    },
    "./token": {
      "types": "./dist/token.d.ts",
      "import": "./dist/token.mjs",
      "require": "./dist/token.js"
    }
  }
}
